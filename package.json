{
  "name": "audio-to-text",
  "version": "0.1.0",
  "author": {
    "name": "Hugo Manrique",
    "url": "https://hugmanrique.me",
    "email": "npm@hugmanrique.me"
  },
  "dependencies": {
    "@google-cloud/speech": "0.9.1",
    "@google-cloud/storage": "1.0.0",
    "node-record-lpcm16": "0.3.0",
    "yargs": "7.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1"
